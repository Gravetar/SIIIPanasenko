@model Traffic
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div>
            Плотность трафика
            <p>@Html.EditorFor(model => model.Density)</p>
        </div>

        <div>
            Путь к файлу трафика
            <p>@Html.EditorFor(model => model.PathFile)</p>
        </div>

        <p><input type="submit" value="Сохранить" /></p>
        @Html.ActionLink("Назад", "Index", "Traffic")
    }
</div>